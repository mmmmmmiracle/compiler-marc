int max(int x, int y) {
    if ((x(int) >= y(int))) {
        return x(int);
    }
    else {
        return y(int);
    }
}

void main() {
    int x;
    int y;
    cout << "enter x value: ";
    cin >> x(int);
    cout << "enter y value: ";
    cin >> y(int);
    z = max(int,int->int)(x(int), y(int));
    cout << "the maximum is ";
    cout << z;
}

